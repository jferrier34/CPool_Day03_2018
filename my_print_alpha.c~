void my_putchar(char c)
{
    write(1, &c, 1);
}

int my_print_alpha(void)
{
    char lettre;
    lettre = 97;
    while (lettre != 122)
    {
	my_putchar(lettre);
	lettre = lettre +1;
    }
}

int main(void)
{
    my_print_alpha();
    return 0;
}

int my_putchar(char c)
{    
    write (1, &c, 1);
}
